{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "img[loading]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "leading-dot-classlist": "off",
    "detect-css-reflows/composite": "off"
  }
}